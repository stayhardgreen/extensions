    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
  }

  .circular-timer {
    width: 24px;
    height: 24px;
  }

  .circular-timer .tpie {
    width: 50%;
    height: 100%;
    -webkit-transform-origin: 100% 50%;
    position: absolute;
    background: white;
    border: 2px solid #50E3C2;
  }

  .circular-timer .tspinner {
    border-radius: 100% 0 0 100% / 50% 0 0 50%;
    z-index: 200;
    border-right: none;
    -webkit-animation: rota 3s linear infinite;
  }

  .circular-timer:hover .tspinner,
  .circular-timer:hover .tfiller,
  .circular-timer:hover .tmask {
    -webkit-animation-play-state: running;
  }

  .circular-timer .tfiller {
    border-radius: 0 100% 100% 0 / 0 50% 50% 0;
    left: 50%;
    opacity: 0;
    z-index: 100;
    -webkit-animation: opa 3s steps(1, end) infinite reverse;
    border-left: none;
  }

  .circular-timer .tmask {
    width: 50%;
    height: 100%;
    position: absolute;
    background: inherit;
    opacity: 1;
    z-index: 300;
    -webkit-animation: opa 3s steps(1, end) infinite;
  }

  .circular-timer .tdigit {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 301;
    width: 100%;
    text-align: center;
    height: 100%;
    line-height: 23px;
    color: gray;
  }
