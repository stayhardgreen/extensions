html[data-modifier="nodependencies"] {
  min-width: 380px;
  min-height: 600px;
  background: white;
  body {
    background: rgb(55, 60, 73);
    border-radius: 2px;
  }

  #actionClose {
    margin-right: -54px;
    visibility: hidden;
  }
}


html[data-appearance="light"] {
  background: transparent !important;

  body, #homeSidebar, #homeContent .header, .side-section .label {
    background: transparent !important;
  }

  #homeContent .header {
    border-bottom: 1px solid rgba(0,0,0,.1);
  }

  .side-section .items {
    .item {
      &:not(.nohover):hover, &.hover, &.context {
        background: rgba(0,0,0,.06);
      }
    }
  }


  #modal, .loading {
    background: rgba(255,255,255,.5);
  }

  #modal-wrap .modal-container {
    ng-include {
      background: transparent;
      box-shadow: none;
      animation: none;
      -webkit-animation: none;
    }
  }

  input.input-on-black, textarea.input-on-black {
    background: transparent;
  }

  #markup-wrap {
    transition: opacity .2s ease-in-out;
  }
  body.modal-mode #markup-wrap, body.now-loading #markup-wrap {
    opacity: 0;
  }
}