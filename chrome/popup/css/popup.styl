/* helpers */
.text-overflow {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.no-selection {
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.box-sizing {
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}
.pull-left {
  float: left;
}
.pull-right {
  float: right;
}
.clear {
  clear:both;
}
.ng-hide {
  display: none !important;
}
.invisible {
  visibility: hidden;
}
.no-animation {
  animation: none !important;
  -webkit-animation: none !important;
  -moz-animation: none !important;
}
.anim-translate {
  animation: opacityTranslateFlow .25s both ease-in-out;
  -webkit-animation: opacityTranslateFlow .25s both ease-in-out;
  //-moz-animation: opacityTranslateFlow .25s both ease-in-out;
  -moz-animation: none;
}
.anim-translate-top {
  animation: opacityTranslateFlowTop .25s both ease-in-out;
  -webkit-animation: opacityTranslateFlowTop .25s both ease-in-out;
  //-moz-animation: opacityTranslateFlow .25s both ease-in-out;
  -moz-animation: none;
}
.center {
  text-align: center;
}
.ui-match {
  background: yellow;
}



.table {
  display: table;

  .row {
    display: table-row;
  }

  .cell {
    display: table-cell;
  }

  &.table-middle {
    .cell {
      vertical-align: middle;
    }
  }
}
.table-max, .cell-max {
  width: 100%;
}




.markup {
  display: table;
  width: 100%;height:100%;

  > .top, > .content, > .footer {
    width: 100%;
    display: table-row;

    > .cell {
      display: table-cell;
      width: 100%;
    }
  }
  > .top {
    .top-wrap {
      display: table-cell;
      width: 100%;

      .top-wrap-wrap {
        position: relative;
      }
    }

    .top-cover {
      position: absolute;z-index:0;
      top:0;left:0;width:100%;height:100%;
      background-position: top center;
      background-size: cover;
      background-repeat: no-repeat;

      transition: background-image .2s ease-in-out;

      animation: backgroundOpacityFlow .3s both ease-in-out;
      -webkit-animation: backgroundOpacityFlow .3s both ease-in-out;
      //-moz-animation: backgroundOpacityFlow .3s both ease-in-out;
      -moz-animation: none;

      .top-cover-wrap {
        width: 100%;
        height:100%;
        //background: linear-gradient(to bottom, rgba(0,0,0,.4) 0%, #90CEFF 100%);
        @extend .dark-background;
        opacity: .8;
      }
    }
  }
  > .content {
    height: 100%;
    //animation: opacityTranslateFlowRewind .2s both ease-in-out;
    //-webkit-animation: opacityTranslateFlowRewind .2s both ease-in-out;
    //-moz-animation: opacityTranslateFlowRewind .2s both ease-in-out;
    -moz-animation: none;

    &[data-color="white"] {
      background: white;
      //animation: opacityTranslateFlowTop .25s both ease-in-out;
      //-webkit-animation: opacityTranslateFlowTop .25s both ease-in-out;
      //-moz-animation: opacityTranslateFlow .25s both ease-in-out;
      //-moz-animation: none;
    }

    > .cell {
      height: 100%;
    }
  }
  > .footer .footer-wrap {
    @extend .box-sizing;
    padding: 8px padding-one;
    padding-bottom: 5px;
    box-shadow: 0px -1px 0px rgba(255,255,255,.06);

    //text-transform: uppercase;
    font-size: 14px;
    color: rgba(255,255,255,.8);

    .icon {
      vertical-align: middle;

      &:hover {
        opacity:1;
      }
    }

    a {
      color: rgba(255,255,255,.45);

      &:hover {
        color: white;

        .icon {
          opacity:1;
        }
      }
    }
  }
}
