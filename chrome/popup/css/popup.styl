}
body.disable-animation .spinner, .spinner {
  -webkit-animation: rotateplane .8s infinite ease-in-out !important;
  -moz-animation: rotateplane .8s infinite ease-in-out !important;
  animation: rotateplane .8s infinite ease-in-out !important;
}
.spinner img {
  width: 16px;
  height: 16px;
  vertical-align: -5px;
}


.loading {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 19999;
  display:none;

  .spinner {
    position: absolute;top:50%;left:50%;margin-left:-12px;margin-top:-12px;
  }
}



@-moz-keyframes rotateplane {
  0% { -moz-transform: perspective(120px);  }
  50% { -moz-transform: perspective(120px) rotateY(180deg); background: #943be6; }
  100% { -moz-transform: perspective(120px) rotateY(180deg)  rotateX(180deg) }
}

@-webkit-keyframes rotateplane {
  0% { -webkit-transform: perspective(120px);  }
  50% { -webkit-transform: perspective(120px) rotateY(180deg); background: #943be6; }
  100% { -webkit-transform: perspective(120px) rotateY(180deg)  rotateX(180deg) }
}

@keyframes rotateplane {
  0% {
    transform: perspective(120px) rotateX(0deg) rotateY(0deg);
    -webkit-transform: perspective(120px) rotateX(0deg) rotateY(0deg)
  }
  50% {
    background: #943be6;
    transform: perspective(120px) rotateX(-180.1deg) rotateY(0deg);
    -webkit-transform: perspective(120px) rotateX(-180.1deg) rotateY(0deg);
  }
  100% {
    transform: perspective(120px) rotateX(-180deg) rotateY(-179.9deg);
    -webkit-transform: perspective(120px) rotateX(-180deg) rotateY(-179.9deg);
  }
}



@keyframes opacityFlow {
  0% { opacity:0; }
  100% { opacity:1; }
}
@keyframes opacityTranslateFlow {
  0% { transform: translateX(body-width); -webkit-transform: translateX(body-width); -moz-transform:none; /*-moz-transform: translateX(body-width);*/ }
  100% { transform: translateX(0); -webkit-transform: translateX(0); -moz-transform:none; /*-moz-transform: translateX(0);*/ }
}
@keyframes opacityTranslateFlowRewind {
  0% { transform: translateX((body-width*-1)); -webkit-transform: translateX((body-width*-1)); -moz-transform:none; /*-moz-transform: translateX((body-width*-1));*/ }
  100% { transform: translateX(0); -webkit-transform: translateX(0); -moz-transform:none; /*-moz-transform: translateX(0);*/ }
}
