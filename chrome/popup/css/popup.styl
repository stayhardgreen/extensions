@-webkit-keyframes rota {
  0% {
    -webkit-transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
  }
}
@-webkit-keyframes opa {
  0% {
    opacity: 1;
  }
  50%, 100% {
    opacity: 0;
  }
}


@keyframes rotateOut {
  0% {
    transform: translateY(0px);
    -webkit-transform: translateY(0px);
    -moz-transform: translateY(0px);
  }
  100% {
    transform: translateY(120px);
    -webkit-transform: translateY(120px);
    -moz-transform: translateY(120px);
  }
}
@keyframes rotateIn {
  0% {
    transform: translateY(-120px) perspective(120px) rotateX(180deg) rotateY(0);
    -webkit-transform: translateY(-120px) perspective(120px) rotateX(180deg) rotateY(0);
    -moz-transform: translateY(-120px) perspective(120px) rotateX(180deg) rotateY(0);
  }
  100% {
    transform: translateY(0) perspective(120px) rotateX(0deg) rotateY(0deg);
    -webkit-transform: translateY(0) perspective(120px) rotateX(0deg) rotateY(0deg);
    -moz-transform: translateY(-120px) perspective(120px) rotateX(180deg) rotateY(0);
  }
}
@keyframes flyIn {
  0% {
    transform: scale(0);
    -webkit-transform: scale(0);
    -moz-transform: scale(0);
  }
  60% {
    transform: scale(1.2);
    -webkit-transform: scale(1.2);
    -moz-transform: scale(1.2);
  }
  80% {
    transform: scale(0.7);
    -webkit-transform: scale(0.7);
    -moz-transform: scale(0.7);
  }
  100% {
    transform: scale(1);
    -webkit-transform: scale(1);
    -moz-transform: scale(1);
  }
}



